@{
    ViewBag.Title = "Home Page";
}

<div class="row">
    @model Mememory.Models.ImageModel

        @foreach (var image in Model)
        {

            <div class="@image.genre col-xs-12 col-sm-6 col-md-4 col-lg-3 image-unit">
                
                <div class="image-box">
                    <a class="remove-image" href="#">Delete</a>
                    <div>
                        <a href="../../Images/@image.url "><img class="image-frame" alt="@image.title" src="../../Images/@image.url " /></a>
                    </div>
                    <div class="title-box">
                        <p>@image.title</p>
                        <p>@image.description</p>
                    </div>
                </div>
                
            </div>
        }
</div>
    
