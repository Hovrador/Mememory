@{
    ViewBag.Title = "Home Page";
}

<div class="row">
    @model Mememory.Models.ImageModel

        @foreach (var image in Model)
        {
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3">
                <a href="../../Images/@image.url "><img src="../../Images/@image.url " height="200" width="200" /></a>
            </div>
        }
</div>
    
